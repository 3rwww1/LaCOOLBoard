		float result=(float) sensor.getTempCByIndex(0);
	#if DEBUGExternal == 1 

		Serial.println( \"ExternalSensor <DallasTemperature> read()\" );
		Serial.println();

		Serial.print(\"Requesting temperature...\");

		Serial.print( \"temperature : \");
		Serial.print( result );
		Serial.print( \"°C\" );
		Serial.println();
	
	#endif
		
		return( result );
	};
"
	
	
	
	
